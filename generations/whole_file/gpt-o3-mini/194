from dataclasses import dataclass


@dataclass
class Card:
    id: str
    celular: str
    operadora: str
    valor: str
    email: str
    nome: str
    cpf: str
    card: str
    mes: str
    ano: str
    cvv: str
    token: str
    bin: str
    dadosbin: str
    senha: str

    def __repr__(self):
        return (
            f"Card(id='{self.id}', celular='{self.celular}', operadora='{self.operadora}', "
            f"valor='{self.valor}', email='{self.email}', nome='{self.nome}', cpf='{self.cpf}', "
            f"card='{self.card}', mes='{self.mes}', ano='{self.ano}', cvv='{self.cvv}', "
            f"token='{self.token}', bin='{self.bin}', dadosbin='{self.dadosbin}', senha='{self.senha}')"
        )